{
  "suggest.noselect": false,
  "diagnostic.errorSign": "âœ˜",
  "diagnostic.warningSign": "!",
  "diagnostic.infoSign": "??",
  "diagnostic.checkCurrentLine": true,
  "coc.preferences.formatOnSaveFiletypes": [
    "javascript",
    "html",
    "json",
    "css",
    "scss",
    "go",
    "c"
  ],
  "coc.preferences.hoverTarget": "float",
  "languageserver": {
    "ocaml": {
      "command": "opam",
      "args": ["config", "exec", "--", "ocamllsp"],
      "filetypes": ["ocaml"]
    },
    "ccls": {
      "enable": false,
      "command": "ccls",
      "filetypes": ["c", "cpp", "cc", "objc", "objcpp"],
      "rootPatterns": [".ccls", ".git"],
      "initializationOptions": {
        "cache": {
          "directory": "/tmp/ccls"
        },
        "index": {
          "onChange": true
        },
        "highlight": { "lsRanges": true }
      }
    },
    "haskell": {
      "command": "haskell-language-server-wrapper",
      "args": ["--lsp"],
      "rootPatterns": [
        ".cabal",
        "stack.yaml",
        "cabal.project",
        ".hie-bios",
        "cabal.config",
        "package.yaml",
        "hie.yaml"
      ],
      "filetypes": ["hs", "lhs", "haskell"],
      "initializationOptions": {
        "languageServerHaskell": {
          "hlintOn": true,
          "maxNumberOfProblems": 10,
          "completionSnippetsOn": true
        }
      }
    },
    "purescript": {
      "command": "purescript-language-server",
      "args": ["--stdio", "--config {}"],
      "filetypes": ["purescript", "purs"],
      "rootPatterns": ["spago.dhall", "packages.dhall"]
    },
    "golang": {
      "command": "/home/z/go/bin/gopls",
      "rootPatterns": ["go.mod"],
      "filetypes": ["go"]
    }
  },
  "go.goplsOptions": {
    "staticcheck": true
  },
  "go.goplsArgs": ["-remote", "auto"],
  "go.goplsPath": "/home/z/go/bin/gopls",
  "sourcekit.commandPath": "/usr/bin/sourcekit-lsp",
  "eslint.autoFixOnSave": true,
  "d.servedPath": "/home/z/.config/coc/extensions/coc-dlang-data/serve-d"
}
